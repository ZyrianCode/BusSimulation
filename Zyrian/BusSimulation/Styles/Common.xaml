<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <!--Colors-->
    <Color x:Key="Color1">#FF1B263C</Color>
    <Color x:Key="Color2">#FF062B48</Color>
  <!--Brushes-->
  <SolidColorBrush x:Key="BrushColor1" Color="{StaticResource Color1}"> </SolidColorBrush>
  <SolidColorBrush x:Key="BrushColor2" Color="{StaticResource Color2}"> </SolidColorBrush>

</ResourceDictionary>